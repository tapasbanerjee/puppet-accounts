<?xml version= '1.0' encoding= 'UTF-8' ?>
<wsdl:definitions
     name="UpdateTaskActivityWS"
     targetNamespace="http://oracle.com/sca/soapservice/AtradiusUtilityApplication2/UpdateTaskActivityService/UpdateTaskActivityWS"
     xmlns:tns="http://oracle.com/sca/soapservice/AtradiusUtilityApplication2/UpdateTaskActivityService/UpdateTaskActivityWS"
     xmlns:inp1="https://group.atradius.com"
     xmlns:inp2="http://xmlns.oracle.com/singleString"
     xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
    >
    <wsdl:types>
        <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="https://group.atradius.com" schemaLocation="../Schemas/TaxDutyBond.xsd"/>
        </xsd:schema>
        <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://xmlns.oracle.com/singleString" schemaLocation="../Schemas/singleString.xsd"/>
        </xsd:schema>
    </wsdl:types>
    <wsdl:message name="requestMessage">
        <wsdl:part name="payload" element="inp1:TaxDutyBond"/>
    </wsdl:message>
    <wsdl:message name="replyMessage">
        <wsdl:part name="payload" element="inp1:processResponse"/>
    </wsdl:message>
    <wsdl:message name="faultMessage">
        <wsdl:part name="faultPayload" element="inp2:singleString"/>
    </wsdl:message>
    <wsdl:portType name="execute_ptt">
        <wsdl:operation name="execute">
            <wsdl:input message="tns:requestMessage"/>
            <wsdl:output message="tns:replyMessage"/>
            
        </wsdl:operation>
    </wsdl:portType>
</wsdl:definitions>
